
<html>
<head>
	<title>HCI Project Portfolio</title>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link href="/css/bootstrap.min.css" rel="stylesheet">
	<link href="/css/bootstrap-theme.min.css" rel="stylesheet">
	<link href="/css/introHCI.css" rel="stylesheet">
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
	<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
	<!-- this is a comment in HTML -->

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->

<!--FB account log in -->
	<script src="https://code.jquery.com/jquery.js"></script>
	<script src="/js/bootstrap.min.js"></script>
	<script src="/js/introHCI.js"></script>
	{{!-- {{> facebook}}
	<script src="/js/facebook.js"></script>
	<script src="/js/loadStuff.js"></script> --}}

</head>

<body>

	<div class="container">
		<!-- h1 through h6 are headers. The higher the number, the smaller the header -->
		<!-- p means paragraph -->
		
		<!--fixed menu top--> 
			<!--Navbar-->


		<div class="jumbotron fixed-top">
			<h1 id="name">&nbsp;&nbsp;&nbsp;&nbsp;Activity log</h1>
			<div class="w-80 top-button fixed-top">
					<div class="btn-group">
						<button type="button" class="btn btn-secondary dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" >
							<svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" fill="#F59292" class="bi bi-grid-fill" viewBox="0 0 16 16">
								<path d="M1 2.5A1.5 1.5 0 0 1 2.5 1h3A1.5 1.5 0 0 1 7 2.5v3A1.5 1.5 0 0 1 5.5 7h-3A1.5 1.5 0 0 1 1 5.5v-3zm8 0A1.5 1.5 0 0 1 10.5 1h3A1.5 1.5 0 0 1 15 2.5v3A1.5 1.5 0 0 1 13.5 7h-3A1.5 1.5 0 0 1 9 5.5v-3zm-8 8A1.5 1.5 0 0 1 2.5 9h3A1.5 1.5 0 0 1 7 10.5v3A1.5 1.5 0 0 1 5.5 15h-3A1.5 1.5 0 0 1 1 13.5v-3zm8 0A1.5 1.5 0 0 1 10.5 9h3a1.5 1.5 0 0 1 1.5 1.5v3a1.5 1.5 0 0 1-1.5 1.5h-3A1.5 1.5 0 0 1 9 13.5v-3z"/>
							</svg>
						</button>
						<div class="dropdown-menu dropdown-menu-right">
						<a href="/index"> <button class="dropdown-item active" type="button">Home</button> </a>
                            <a href="/setting"> <button class="dropdown-item" type="button">Setting</button></a>
							<a href="/about"> <button class="dropdown-item" type="button">About</button></a>
							<a href="/logIn"> <button class="dropdown-item" type="button" style="background-color: grey; color: white;">Log Out</button></a>
						</div>
					</div>
			</div>
		</div>
		
		<!-- log in w/facebook
			<p class="facebookLogin">Please login through Facebook</p>
			<fb:login-button id="fb_button" scope="public_profile,email" onlogin="checkLoginState();">
			</fb:login-button>
		

		<div id="facebookPhoto" class="profile">
			<h3>Profile Photo</h3>
			<img id="photo" class='img-responsive' src="http://hci.stanford.edu/courses/cs547/Resources/Pictures/klemmer_grad.jpg" />
		</div>
		--> 

		<!-- divs are invisible structuring elements that stack vertically by default. Use them to organize your code -->
		<!-- img tags are images. Lorem Pixel will deliver random images; handy! -->
		<!-- a are anchors, also known as hyperlinks. Use the href attribute to tell the browser where to go when the user clicks -->
		<br><br><br><br><br><br><br><br>

		{{#each toLog}}
		<div class="card w-80" id="card">
			<div class="card-body" >
				<div class="card-iteminline">

					<h2 class="card-title">{{Activity}}</h2>
					<div id="share-link">
						<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="currentColor" class="bi bi-share"
							viewBox="0 0 16 16">
						
							<path
								d="M13.5 1a1.5 1.5 0 1 0 0 3 1.5 1.5 0 0 0 0-3zM11 2.5a2.5 2.5 0 1 1 .603 1.628l-6.718 3.12a2.499 2.499 0 0 1 0 1.504l6.718 3.12a2.5 2.5 0 1 1-.488.876l-6.718-3.12a2.5 2.5 0 1 1 0-3.256l6.718-3.12A2.5 2.5 0 0 1 11 2.5zm-8.5 4a1.5 1.5 0 1 0 0 3 1.5 1.5 0 0 0 0-3zm11 5.5a1.5 1.5 0 1 0 0 3 1.5 1.5 0 0 0 0-3z" />
						</svg>
					</div>
				</div>
				<h3 class="card-subtitle mb-2 text-muted" >{{minTime}} minutes</h3>
			</div>
				<img class="card-img-top" src="{{Image}}" alt="Unsplash image">
			<div class="card-body">
				<div class="details card-text">
					<h4>{{Description}}</h4>
				</div>
				<br>
				<div class="card-iteminline">
					<a href="#" class="card-link" id="delete">Delete</a>
					<a href="/activity" class="btn btn-primary">DO IT AGAIN</a> 
				</div>
					
			</div>
		</div>
		{{/each}}

		<!--button nav-->
		<a href="/filter">
			<div class="fixed-bottom">
					<div class="home-bottom">
						<button type="button" class="btn-primary w-75 primarybtn">Get new idea!</button>
					</div>
			</div>
		</a>
		
	</div>

	<script>
		var button = document.querySelector("#delete");
		var ele = document.querySelector("#card");
		button.addEventListener('click', function() {
			ele.parentNode.removeChild(ele);
		})

		const sharebtn = document.getElementById('share-link');
		sharebtn.addEventListener('click', function (e) {
			
			alert('Coming Soon!');
		});

	</script>

</body>
</html>
